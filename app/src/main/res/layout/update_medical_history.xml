<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    >

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/Theme.Experiment.AppBarOverlay">

    </com.google.android.material.appbar.AppBarLayout>

    <Button
        android:layout_width="384dp"
        android:layout_height="95dp"
        android:text="Medical Report\n\nDate: 11/04/2023 Time: 11:00 AM"
        android:textColor="#FFFF"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="116dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="384dp"
        android:layout_height="95dp"
        android:text="Medical Report\n\nDate: 21/01/2023 Time: 12:30 AM"
        android:textColor="#FFFF"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="233dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="384dp"
        android:layout_height="95dp"
        android:text="Medical Report\n\nDate: 16/12/2022 Time: 6:50 PM"
        android:textColor="#FFFF"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="357dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="384dp"
        android:layout_height="95dp"
        android:text="Medical Report\n\nDate: 7/10/2022 Time: 1:45 PM"
        android:textColor="#FFFF"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="481dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        app:backgroundTint="#03A9F4"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:layout_editor_absoluteX="325dp"
        tools:layout_editor_absoluteY="646dp" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="88dp"
        android:background="?attr/colorPrimary"
        android:textColor="#FFFF"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="6dp"
        android:text="Update Medical History"
        android:textSize="25dp"
        android:textColor="#FFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="16dp" />


</androidx.constraintlayout.widget.ConstraintLayout>